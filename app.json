{
  "expo": {
    "name": "SwiftStock",
    "slug": "swiftstock-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "icon": "./assets/swiftstock.png", 
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "permissions": [
        "CAMERA"
      ],
      "package": "com.efendi7.swiftstock"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Izinkan SwiftStock mengakses kamera untuk scan barcode produk."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8546c331-3c3a-4ca4-8fca-094f500b17f1"
      }
    }
  }
}